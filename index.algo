ALGORITHM Dot_product:
VAR
    ps: FLOAT
    v1, v2 : ARRAY_OF float[2]
BEGIN
    FUNCTION Dot_product(v1, v2: ARRAY_OF float): float
    BEGIN
       ps:=V1[0]*V2[0] + V1[1]* V2[1]
    END
    write("execute the two vertors")
    read(v1, v2);
    ps:=Dot_product(v1,v2)
    if(ps=0) THEN 
    write("both are ortogonals")
    Else
    write("both are NOT ortogonals")
    END IF
END